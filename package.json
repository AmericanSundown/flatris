{
  "name": "flatris",
  "license": "MIT",
  "version": "1.0.0",
  "description": "A fast-paced two-player web game",
  "repository": {
    "type": "git",
    "url": "https://github.com/skidding/flatris.git"
  },
  "scripts": {
    "lint": "eslint '**/*.js' '!**/.next/**'",
    "format": "prettier --write '**/*.{js,css}' '!**/{flow-typed,.next}/**'",
    "test": "flow && yarn lint && jest --maxWorkers=4",
    "test:watch": "jest --watch",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "dev:client": "babel-node server/start-dev-client.js",
    "dev:server": "babel-node server/start-dev-server.js",
    "dev:server:watch": "nodemon server/start-dev-server.js  --exec babel-node",
    "build": "next build",
    "start": "babel-node server/start-prod.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "crc-32": "^1.2.0",
    "express": "^4.16.3",
    "firebase-admin": "^5.12.1",
    "isomorphic-unfetch": "^2.0.0",
    "lodash": "^4.17.10",
    "next": "^5.1.0",
    "next-redux-wrapper": "^1.3.5",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "rollbar": "^2.4.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.0.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.12.1",
    "react-cosmos": "^4.4.0",
    "react-cosmos-fetch-proxy": "^4.4.0",
    "react-cosmos-redux-proxy": "^4.4.0",
    "react-cosmos-telescope": "^4.4.0",
    "react-test-renderer": "^16.4.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.21.0"
  }
}
